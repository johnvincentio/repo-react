{
	"name": "hooks-project",
	"version": "1.0.0",
	"description": "React project using hooks",
	"repository": {
		"type": "git",
		"url": "git://git@github.com:johnvincentio/change-repo.git"
	},
	"main": "index.js",
	"jest": {
		"roots": [
			"<rootDir>/src/",
			"<rootDir>/tests/__tests__/"
		],
		"testRegex": "./tests/__tests__/.*.(js|jsx)$",
		"setupFilesAfterEnv": [
			"<rootDir>/tests/setupTests.js"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/tests/__tests__/setup/"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
			"\\.(css|less|scss)$": "<rootDir>/tests/__mocks__/styleMock.js"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not ie <= 11",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jv": {
		"test-one": "npm test -- -t '<One />'",
		"test-help": "jest --help",
		"test": "jest",
		"production-statistics": "rm -rf dist && NODE_ENV=production webpack --mode production --profile --json > production-statistics.json && webpack-bundle-analyzer production-statistics.json dist",
		"statistics": "webpack --mode development --profile --json > statistics.json && webpack-bundle-analyzer statistics.json dist",
		"production": "rm -rf dist && NODE_ENV=production npm run build",
		"help": "webpack --help",
		"eslint-check": "eslint --print-config . | eslint-config-prettier-check",
		"lint": "eslint 'src/**/*.{js,jsx}' --quiet",
		"eslint-versions": "npm info eslint-config-airbnb@latest peerDependencies"
	},
	"scripts": {
		"dev": "webpack --mode development --progress",
		"start": "webpack-dev-server --hot --inline --mode development",
		"serve-help": "serve --help",
		"serve": "serve -l 8230 dist",
		"build": "webpack --mode production --progress",
		"audit": "npm audit"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"@babel/preset-typescript": "^7.12.7",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.3.2",
		"css-loader": "^5.0.1",
		"enzyme": "^3.11.0",
		"eslint": "^7.15.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^7.0.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^4.5.0",
		"jest": "^26.6.3",
		"mini-css-extract-plugin": "^1.3.2",
		"node-sass": "^5.0.0",
		"normalize-scss": "^7.0.1",
		"sass-loader": "^10.1.0",
		"serve": "^11.3.2",
		"style-loader": "^2.0.0",
		"sw-precache-webpack-plugin": "^1.0.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"underscore": "^1.12.0",
		"webpack": "^4.44.2",
		"webpack-bundle-analyzer": "^4.2.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-manifest-plugin": "^3.0.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.12.5",
		"@material-ui/core": "^4.11.2",
		"@material-ui/icons": "^4.11.2",
		"axios": "^0.21.0",
		"classnames": "^2.2.6",
		"dotenv": "^8.2.0",
		"moment": "^2.29.1",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dnd": "^11.1.3",
		"react-dnd-html5-backend": "^11.1.3",
		"react-dom": "^17.0.1",
		"react-loadable": "^5.5.0",
		"react-redux": "^7.2.2",
		"react-responsive": "^8.2.0",
		"react-router-dom": "^5.2.0",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"styled-components": "^5.2.1",
		"uuid": "^8.3.1"
	}
}
